<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
		package="org.me.openglapplication">
  <application 
        android:label="@string/app_name"
        android:icon="@drawable/icon">
    <activity android:label="@string/app_name" android:name="org.me.openglapplication.MainActivity">
      <intent-filter>
        <action android:name="android.intent.action.MAIN" />
        <category android:name="android.intent.category.LAUNCHER" />
      </intent-filter>
    </activity>
    <activity android:label="@string/openGL_activity" android:name="org.me.openglapplication.OpenGLActivity"
              android:theme="@android:style/Theme.Translucent" />
    <activity android:label="@string/about_activity" android:name="org.me.openglapplication.AboutActivity"
              android:theme="@android:style/Theme.Dialog" />
  </application>
  <uses-sdk android:minSdkVersion="4" />
</manifest>
