<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
		package="org.me.tabbedapp">
  <application 
        android:label="@string/app_name"
        android:icon="@drawable/icon">
    <activity android:label="@string/app_name" android:name="org.me.tabbedapp.MainActivity"
              android:theme="@android:style/Theme.NoTitleBar">
      <intent-filter>
        <action android:name="android.intent.action.MAIN" />
        <category android:name="android.intent.category.LAUNCHER" />
      </intent-filter>
    </activity>
    <activity android:label="@string/tab_one_label" android:name="org.me.tabbedapp.TabOneActivity" />
    <activity android:label="@string/tab_two_label" android:name="org.me.tabbedapp.TabTwoActivity" />
    <activity android:label="@string/tab_three_label" android:name="org.me.tabbedapp.TabThreeActivity" />
  </application>
  <uses-sdk android:minSdkVersion="4" />
</manifest>
